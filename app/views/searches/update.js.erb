{
  "status": "ok"
  <% if @new_timestamp %>
  ,
  "timestamp": <%= @new_timestamp %>
  <% end %>
  <% if params[:page] && @search.phase == "exploratory" %>
  ,
  "pages": <%= @search.pages.to_json %>
  <% end %>
  <% if params[:level] && @search.phase == "exploratory" && @search.pages == (0..(Search::MAX_XP_PAGES - 1)).entries %>
  ,
  "xpTiles": <%= @search.xpTiles.to_json.html_safe %>
  <% end %>
  <% if @new_block %>
  ,
  "boxes": <%= @new_block.blank? ? nil.to_json : @new_block.to_json.html_safe %>
  ,
  "level": <%= params[:level] %>
  <% end %>
  <% if params[:benchmarks] %>
  ,
  "benchmarks": <%= @search.client_benchmarks.to_json.html_safe %>
  <% end %>
}